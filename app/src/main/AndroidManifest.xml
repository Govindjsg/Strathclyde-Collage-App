<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.govind.strathclydeuser" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.GHSNayanagr"
        android:name="com.govind.strathclydeuser.Strathclyde">

        <activity android:name="com.govind.strathclydeuser.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.govind.strathclydeuser.authencation.ForgetPasswordActivity" />
        <activity android:name="com.govind.strathclydeuser.authencation.LoginActivity" />
        <activity android:name="com.govind.strathclydeuser.authencation.RegisterActivity" />
        <activity android:name="com.govind.strathclydeuser.developer.Developer" />
        <activity android:name="com.govind.strathclydeuser.FullImageView" />
        <activity
            android:name="com.govind.strathclydeuser.ebook.PdfViwerActivity"
            android:theme="@style/Custom" />
        <activity
            android:name="com.govind.strathclydeuser.ebook.EbookActivity"
            android:parentActivityName="com.govind.strathclydeuser.MainActivity" />
        <activity android:name="com.govind.strathclydeuser.MainActivity" />


    </application>

</manifest>